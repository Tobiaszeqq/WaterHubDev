coroutine.wrap(IEQWUKK_fake_script)()
local function BWSW_fake_script() -- TextButton.espscriptnoway 
	local script = Instance.new('LocalScript', TextButton)

	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local Button = script.Parent 
	
	local function highlightPlayer(character)
		if character:FindFirstChild("HighlightEffect") then return end 
	
		local highlight = Instance.new("Highlight")
		highlight.Name = "HighlightEffect"
		highlight.Adornee = character
		highlight.FillColor = Color3.fromRGB(0, 255, 0)
		highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
		highlight.Parent = character
	end
	
	local function highlightAllPlayers()
		for _, player in pairs(Players:GetPlayers()) do
			if player ~= LocalPlayer and player.Character then
				highlightPlayer(player.Character)
			end
		end
	end
	
	Button.MouseButton1Click:Connect(function()
		highlightAllPlayers()
	end)
	
	
end
coroutine.wrap(BWSW_fake_script)()
local function ABJCR_fake_script() -- trail.trailnoway 
	local script = Instance.new('LocalScript', trail)

	-- it supposed to be a china hat but idk how to make one
	--  100% code is not from chatgpt :)
	
	local player = game.Players.LocalPlayer
	
	local button = script.Parent
	
	local function createHatEffect()
		local character = player.Character or player.CharacterAdded:Wait()
		local head = character:WaitForChild("Head")
	
		if head:FindFirstChild("HatEffect") then
			warn("trail is already enabled")
			return
		end
	
		local particleEmitter = Instance.new("ParticleEmitter")
		particleEmitter.Name = "HatEffect"
		particleEmitter.Parent = head
	
		particleEmitter.Texture = "rbxassetid://247944275"
		particleEmitter.Rate = 50
		particleEmitter.Lifetime = NumberRange.new(0.5, 0.7)
		particleEmitter.Speed = NumberRange.new(0, 0)
		particleEmitter.RotSpeed = NumberRange.new(0, 360) 
		particleEmitter.Size = NumberSequence.new({
			NumberSequenceKeypoint.new(0, 0), 
			NumberSequenceKeypoint.new(0.5, 3),
			NumberSequenceKeypoint.new(1, 0) 
		})
		particleEmitter.Transparency = NumberSequence.new({
			NumberSequenceKeypoint.new(0, 0.5),
			NumberSequenceKeypoint.new(1, 1) 
		})
		particleEmitter.Color = ColorSequence.new(Color3.new(0.258824, 1, 1)) 
		particleEmitter.EmissionDirection = Enum.NormalId.Top
	
		particleEmitter.Enabled = true
	end
	
	button.MouseButton1Click:Connect(createHatEffect)
	
end
coroutine.wrap(ABJCR_fake_script)()
local function HUAODY_fake_script() -- scripts.opengui 
	local script = Instance.new('LocalScript', scripts)

	local UserInputService = game:GetService("UserInputService")
	
	local frame = script.Parent.Parent:WaitForChild("main")
	local isFrameVisible = false 
	
	local function toggleFrame()
		isFrameVisible = not isFrameVisible
		frame.Visible = isFrameVisible
	end
	
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
	
		if input.KeyCode == Enum.KeyCode.LeftControl then
			toggleFrame()
		end
	end)
	
end
coroutine.wrap(HUAODY_fake_script)()
local function VFOD_fake_script() -- notify.notify 
	local script = Instance.new('LocalScript', notify)

	local StarterGui = game:GetService("StarterGui")
	
	local function showNotification(title, text, iconId)
		StarterGui:SetCore("SendNotification", {
			Title = title or "Achievement Unlocked!", 
			Text = text or "You have unlocked something special!", 
			Duration = 5 
		})
	end
	
	showNotification(
		"WaterHub Lite",
		"To open gui click left CTRL"
	)
	
	
end
coroutine.wrap(VFOD_fake_script)()
