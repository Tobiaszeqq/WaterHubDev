-- Scripts

local function JEGUVE_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local textLabel = script.Parent -- Upewnij się, że skrypt jest dzieckiem TextLabel
	local transitionSpeed = 0.01 -- Im mniejsza liczba, tym szybsza zmiana
	local lightBlue = Color3.fromRGB(173, 216, 230) -- Jasny niebieski (LightBlue)
	local darkBlue = Color3.fromRGB(0, 0, 139) -- Ciemny niebieski (DarkBlue)
	
	-- Funkcja płynnej zmiany koloru
	local function lerpColor(color1, color2, alpha)
		return Color3.new(
			color1.R + (color2.R - color1.R) * alpha,
			color1.G + (color2.G - color1.G) * alpha,
			color1.B + (color2.B - color1.B) * alpha
		)
	end
	
	-- Funkcja zmieniająca kolor płynnie w pętli
	local function changeTextColor()
		while true do
			-- Z jasnego na ciemny
			for i = 0, 1, transitionSpeed do
				textLabel.TextColor3 = lerpColor(lightBlue, darkBlue, i)
				wait(0.01)
			end
			-- Z ciemnego na jasny
			for i = 0, 1, transitionSpeed do
				textLabel.TextColor3 = lerpColor(darkBlue, lightBlue, i)
				wait(0.01)
			end
		end
	end
	
	-- Uruchomienie zmiany koloru
	changeTextColor()
	
end
coroutine.wrap(JEGUVE_fake_script)()
local function UIJDGKP_fake_script() -- main.draggablegui 
	local script = Instance.new('LocalScript', main)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local drag = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			drag = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					drag = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if drag then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(UIJDGKP_fake_script)()
local function RTKS_fake_script() -- infcash.infmoneyxd 
	local script = Instance.new('LocalScript', infcash)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("ReplicatedStorage").events.purchase:FireServer("Flimsy Rod", "Rod", nil, math.huge)
	end)
	
end
coroutine.wrap(RTKS_fake_script)()
local function IEQWUKK_fake_script() -- Lite.LocalScript 
	local script = Instance.new('LocalScript', Lite)

	local textLabel = script.Parent -- Upewnij się, że skrypt jest dzieckiem TextLabel
	local transitionSpeed = 0.01 -- Im mniejsza liczba, tym szybsza zmiana
